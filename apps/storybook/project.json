{
  "name": "storybook",
  "projectType": "application",
  "sourceRoot": "apps/storybook/src",
  "implicitDependencies": ["ui"],
  "targets": {
    "dev": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm exec vite",
        "cwd": "apps/storybook"
      }
    },
    "build": {
      "executor": "nx:run-commands",
      "dependsOn": ["typecheck"],
      "outputs": ["{projectRoot}/dist"],
      "options": {
        "command": "pnpm exec vite build",
        "cwd": "apps/storybook"
      }
    },
    "typecheck": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm exec tsc -b",
        "cwd": "apps/storybook"
      }
    },
    "preview": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm exec vite preview",
        "cwd": "apps/storybook"
      }
    },
    "lint": {
      "executor": "nx:run-commands",
      "outputs": [],
      "options": {
        "command": "pnpm exec eslint .",
        "cwd": "apps/storybook"
      }
    },
    "storybook": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm exec storybook dev -p 6006",
        "cwd": "apps/storybook"
      }
    },
    "build-storybook": {
      "executor": "nx:run-commands",
      "outputs": ["{projectRoot}/storybook-static"],
      "options": {
        "command": "pnpm exec storybook build",
        "cwd": "apps/storybook"
      }
    }
  },
  "tags": ["type:application", "scope:storybook"]
}
