{
  "name": "web",
  "projectType": "application",
  "sourceRoot": "apps/web/src",
  "implicitDependencies": ["aria", "domain", "policies", "runtime", "sdk", "ui", "validation"],
  "targets": {
    "dev": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm exec vite",
        "cwd": "apps/web"
      }
    },
    "build": {
      "executor": "nx:run-commands",
      "dependsOn": ["typecheck"],
      "outputs": ["{projectRoot}/dist"],
      "options": {
        "command": "pnpm exec vite build",
        "cwd": "apps/web"
      }
    },
    "typecheck": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm exec tsc --noEmit --project tsconfig.app.json",
        "cwd": "apps/web"
      }
    },
    "preview": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm exec vite preview",
        "cwd": "apps/web"
      }
    },
    "lint": {
      "executor": "nx:run-commands",
      "outputs": [],
      "options": {
        "command": "pnpm exec eslint .",
        "cwd": "apps/web"
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "outputs": ["{projectRoot}/coverage"],
      "options": {
        "command": "pnpm exec vitest run --coverage",
        "cwd": "apps/web"
      }
    },
    "test-a11y": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm exec vitest run tests/a11y/pages-a11y.spec.ts",
        "cwd": "apps/web"
      }
    },
    "test-e2e": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm exec playwright test",
        "cwd": "apps/web"
      }
    }
  },
  "tags": ["type:application", "scope:web"]
}
