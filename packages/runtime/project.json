{
  "name": "runtime",
  "projectType": "library",
  "sourceRoot": "packages/runtime/src",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "outputs": ["{projectRoot}/dist"],
      "options": {
        "command": "pnpm exec unbuild",
        "cwd": "packages/runtime"
      }
    },
    "dev": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm exec unbuild --stub",
        "cwd": "packages/runtime"
      }
    },
    "typecheck": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm exec tsc -p tsconfig.json",
        "cwd": "packages/runtime"
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "outputs": [],
      "options": {
        "command": "node ./scripts/run-tests.mjs",
        "cwd": "packages/runtime"
      }
    }
  },
  "tags": ["type:library", "scope:runtime"]
}
