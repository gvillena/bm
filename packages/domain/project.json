{
  "name": "domain",
  "projectType": "library",
  "sourceRoot": "packages/domain/src",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "outputs": ["{projectRoot}/dist"],
      "options": {
        "command": "pnpm exec unbuild",
        "cwd": "packages/domain"
      }
    },
    "dev": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm exec unbuild --stub",
        "cwd": "packages/domain"
      }
    },
    "typecheck": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm exec tsc -p tsconfig.json",
        "cwd": "packages/domain"
      }
    }
  },
  "tags": ["type:library", "scope:domain"]
}
