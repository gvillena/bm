{
  "name": "ui",
  "projectType": "library",
  "sourceRoot": "packages/ui/src",
  "implicitDependencies": ["aria", "domain", "policies", "runtime", "sdk", "validation"],
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "outputs": ["{projectRoot}/dist"],
      "options": {
        "command": "pnpm exec unbuild",
        "cwd": "packages/ui"
      }
    },
    "dev": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm exec unbuild --stub",
        "cwd": "packages/ui"
      }
    },
    "playground": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm exec vite",
        "cwd": "packages/ui"
      }
    },
    "typecheck": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm exec tsc -p tsconfig.json",
        "cwd": "packages/ui"
      }
    },
    "lint": {
      "executor": "nx:run-commands",
      "outputs": [],
      "options": {
        "command": "pnpm exec eslint .",
        "cwd": "packages/ui"
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm exec vitest run",
        "cwd": "packages/ui"
      }
    },
    "test-a11y": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm exec vitest run tests/a11y.spec.ts",
        "cwd": "packages/ui"
      }
    },
    "storybook": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm exec storybook dev -p 6006",
        "cwd": "packages/ui"
      }
    },
    "build-storybook": {
      "executor": "nx:run-commands",
      "outputs": ["{projectRoot}/storybook-static"],
      "options": {
        "command": "pnpm exec storybook build",
        "cwd": "packages/ui"
      }
    },
    "format": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm exec prettier --write \"**/*.{ts,tsx,md,css}\"",
        "cwd": "packages/ui"
      }
    }
  },
  "tags": ["type:library", "scope:ui"]
}
