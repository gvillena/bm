{
  "name": "policies",
  "projectType": "library",
  "sourceRoot": "packages/policies/src",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "outputs": ["{projectRoot}/dist"],
      "options": {
        "command": "pnpm exec unbuild",
        "cwd": "packages/policies"
      }
    },
    "dev": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm exec unbuild --stub",
        "cwd": "packages/policies"
      }
    },
    "typecheck": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm exec tsc -p tsconfig.json",
        "cwd": "packages/policies"
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "dependsOn": ["typecheck"],
      "outputs": [],
      "options": {
        "command": "node --test tests/**/*.spec.mjs",
        "cwd": "packages/policies"
      }
    }
  },
  "tags": ["type:library", "scope:policies"]
}
