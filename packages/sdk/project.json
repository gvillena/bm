{
  "name": "sdk",
  "projectType": "library",
  "sourceRoot": "packages/sdk/src",
  "implicitDependencies": ["aria", "domain", "policies", "runtime"],
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "outputs": ["{projectRoot}/dist"],
      "options": {
        "command": "pnpm exec unbuild",
        "cwd": "packages/sdk"
      }
    },
    "dev": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm exec unbuild --stub",
        "cwd": "packages/sdk"
      }
    },
    "typecheck": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm exec tsc -p tsconfig.json",
        "cwd": "packages/sdk"
      }
    },
    "lint": {
      "executor": "nx:run-commands",
      "outputs": [],
      "options": {
        "command": "pnpm exec eslint \"src/**/*.ts\"",
        "cwd": "packages/sdk"
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "outputs": ["{projectRoot}/coverage"],
      "options": {
        "command": "pnpm exec vitest run --coverage",
        "cwd": "packages/sdk"
      }
    }
  },
  "tags": ["type:library", "scope:sdk"]
}
